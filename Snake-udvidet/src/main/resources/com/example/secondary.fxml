<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #AA0751;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.SecondaryController">
    <children>
        <Label alignment="CENTER" minHeight="223.0" minWidth="600.0" prefHeight="223.0" prefWidth="600.0" style="-fx-background-color: 500;" text="Game Over" textFill="WHITE">
         <font>
            <Font name="Algerian" size="59.0" />
         </font></Label>
        <Button fx:id="secondaryButton" minWidth="40.0" onAction="#switchToPrimary" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #60042f;" text="Try Again" textFill="WHITE">
         <font>
            <Font name="Algerian" size="29.0" />
         </font></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
